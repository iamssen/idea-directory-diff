{
  "name": "idea-directory-diff",
  "displayName": "Idea Directory Diff",
  "description": "Execute shell command 'idea diff {dir} {dir}' in VSCode",
  "version": "0.0.1",
  "publisher": "Lee Seoyeon",
  "engines": {
    "vscode": "^1.26.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iamssen/idea-directory-diff.git"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:ssenkit.ideaDiffDirectories"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "ssenkit.ideaDiffDirectories",
        "title": "Compare Directories with idea diff"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "when": "explorerResourceIsFolder",
          "command": "ssenkit.ideaDiffDirectories",
          "title": "Compare Directories with idea diff",
          "group": "z_commands"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.6.1",
    "vscode": "^1.1.21",
    "tslint": "^5.8.0",
    "@types/node": "^8.10.25",
    "@types/mocha": "^2.2.42"
  }
}